    "@typescript-eslint/parser": "^6.20.0",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "eslint-plugin-mocha": "^10.2.0",
    "markdownlint-cli": "^0.37.0",
    "mocha": "^8.4.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"

{
  "name": "langs-es",
  "version": "3.0.0",
  "description": "ISO 639-1/2/3 Language codes with English and local names",
  "keywords": [
    "639",
    "639-1",
    "639-2",
    "639-3",
    "iso",
    "iso639",
    "lang",
    "langs",
    "language",
    "languages",
    "es",
    "esm",
    "ts"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
